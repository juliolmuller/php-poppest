{
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development": "yarn run dev",
    "watch": "yarn run development -- --watch",
    "watch:poll": "yarn run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "yarn run prod",
    "publish": "heroku login && git push heroku master"
  },
  "dependencies": {
    "@lacussoft/eslint-config-standard": "^1.0.7",
    "@types/jquery": "^3.5.1",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "eslint": "^7.8.1",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "cross-env": "^7.0.2",
    "heroku": "^7.42.13",
    "laravel-mix": "^5.0.5",
    "laravel-mix-react-typescript-extension": "^1.0.3",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "typescript": "^4.0.2"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "@lacussoft/standard"
    ],
    "rules": {}
  }
}
